<div id="select-ref-list-modal" class="modal" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">×</button>
                <h4 class="modal-title">リンクの移動</h4>
            </div>
            <div class="modal-body">
                <label>移動先のリスト</label>
                <select class="form-control" size="10"
                        ng-model="MoveTargetRefList" ng-options="list as list.Title for list in RefLists">
                </select>
            </div>
            <div class="modal-footer">
                <button class="btn btn-primary" type="button" data-dismiss="modal" aria-hidden="true">OK</button>
                <button class="btn btn-default" type="button" data-dismiss="modal" aria-hidden="true">キャンセル</button>
            </div>
        </div>
    </div>
</div>
